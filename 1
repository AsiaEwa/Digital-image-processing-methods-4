close all;
%wczytanie obrazu
A = imread('lena.png');
% czarno-bia≈Çy obraz leny
B = rgb2gray(A);
figure(1);
imshow(B);
% obraz z szumem Gaussa
C = imnoise(B,'gaussian');
figure(2);
imshow(C);
% obraz z szumem sol i pieprz
D = imnoise(B,'salt & pepper');
figure(3);
imshow(D);
% obraz z szumem Poissona
E = imnoise(B,'poisson');
figure(4);
imshow(E);
% obraz z szumem speckle
F = imnoise(B,'speckle');
figure(5);
imshow(F);
